{
    "name": "sudo/laravel-analytics",
    "description": "A Laravel package to retrieve Google Analytics data.",
    "keywords": [
        "spatie",
        "google",
        "analytics",
        "retrieve",
        "reports",
        "laravel"
    ],
    "homepage": "https://github.com/phongtrank55/laravel-analytics",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.2",
        "google/apiclient" : "^2.0",
        "laravel/framework": "^7.0",
        "madewithlove/illuminate-psr-cache-bridge": "^1.0",
        "google/analytics-data": "^0.9.4",
        "nesbot/carbon": "^1.21 | ^2.0"

      },
      "require-dev": {
        "league/flysystem": ">=1.0.8",
        "mockery/mockery": "1.0",
        "orchestra/testbench" : "~3.8.0"

      },
    "autoload": {
        "psr-4": {
            "Spatie\\Analytics\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Analytics\\AnalyticsServiceProvider"
            ],
            "aliases": {
                "Analytics": "Spatie\\Analytics\\Facades\\Analytics"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
